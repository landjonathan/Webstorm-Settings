<application>
  <component name="EditorSettings">
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="b10025" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern pattern="\bprod\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\bmaybe\b.*">
      <option name="FOREGROUND" value="848891" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
  </component>
</application>