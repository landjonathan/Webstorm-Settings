<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="GraphQL" injector-id="xml">
      <display-name>page-query</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("page-query")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
    <injection language="GraphQL" injector-id="xml">
      <display-name>static-query</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("static-query")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>